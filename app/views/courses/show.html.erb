<div class="row">
	<div class="col s12">
		<h3 class="center-align">Course Detail</h3>
	</div>
</div>
<div class="row">
  <div class="col s12 m12">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">Code: <%= @course.course_code%></span>
        <span class="card-title">Name: <%= @course.course_name%></span>
        <p>Description: &nbsp;<%= @course.course_description %></p>
        <div class="card-action"> 
          <%= link_to 'VIEW ALL COURSES', root_path %>  
        </div>
      </div>
    </div>
  </div>
</div>
