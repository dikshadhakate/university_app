<div class="row">
	<div class="col s12">
		<h3 class="center-align">Student Profile</h3>
	</div>
</div>
<div class="row">
    <div class="col s6 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Name: <%= @student.student_name%></span>
          <p>Email: <%= @student.email %></p>
          <p>Course Enrollment: None</p>
        </div>
        <div class="card-action">
          <%= link_to 'EDIT PROFILE', edit_student_path %>
        </div>
      </div>
    </div>
</div>
